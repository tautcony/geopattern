{
  "name": "geopattern",
  "version": "1.2.3",
  "description": "Generate beautiful SVG patterns",
  "keywords": [
    "svg",
    "pattern",
    "geometric",
    "background"
  ],
  "license": "MIT",
  "contributors": [
    "Brandon Mills",
    "Jason Long",
    "TautCony"
  ],
  "main": "lib/index.ts",
  "files": [
    "lib/*.ts"
  ],
  "scripts": {
    "lint": "eslint ./lib/**/*.ts",
    "mocha": "mocha -r ts-node/register tests/**/*.spec.ts",
    "test": "npm run lint && npm run mocha",
    "build": "webpack --config build/webpack.config.js",
    "watch": "watchify -s GeoPattern -i buffer . -p [ tsify --noImplicitAny ] -o js/geopattern.js",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tautcony/geopattern"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jquery": "^3.5.4",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@types/xml-parser": "^1.2.29",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "browserify": "^17.0.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-loader": "^4.0.2",
    "mocha": "^8.2.0",
    "source-map-loader": "^1.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "tsify": "^5.0.2",
    "typescript": "^4.0.5",
    "uglify-js": "^3.11.4",
    "watchify": "^3.11.1",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpackbar": "^4.0.0",
    "xml-parser": "^1.2.1"
  }
}
